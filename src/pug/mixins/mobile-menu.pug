mixin mobile-menu()
	.mobile-menu
		.mobile-menu__panel.mobile-menu__panel--top
			.mobile-menu-services
				.mobile-menu-services__body
					.mobile-menu-services__back
						button.mobile-menu-services__back-button Услуги
					.mobile-menu-services__items
						a.mobile-menu-services__item.cyan-color(href='./consultation.html')
							p.mobile-menu-services__link Консультация и диагностика
							img(src='./images/services/1.png')
						a.mobile-menu-services__item.yellow-color(href='./implantation.html')
							p.mobile-menu-services__link Имплантация
							img(src='./images/services/2.png')
						a.mobile-menu-services__item.light-green-color(href='./veneers.html')
							p.mobile-menu-services__link Виниры
							img(src='./images/services/3.png')
						a.mobile-menu-services__item.lavender-color(href='#')
							p.mobile-menu-services__link Коронки
							img(src='./images/services/4.png')
						a.mobile-menu-services__item.purple-color(href='#')
							p.mobile-menu-services__link Установка брекетов
							img(src='./images/services/5.png')
						a.mobile-menu-services__item.light-lilac-color(href='#')
							p.mobile-menu-services__link Установка элайнеров
							img.reverse-img(src='./images/services/6.png')
						a.mobile-menu-services__item.yellow-color(href='#')
							p.mobile-menu-services__link Лечение зубов
							img(src='./images/services/7.png')
						a.mobile-menu-services__item.cyan-color(href='#')
							p.mobile-menu-services__link Профессиональная гигиена
							img(src='./images/services/8.png')
						a.mobile-menu-services__item.light-green-color(href='#')
							p.mobile-menu-services__link Профессиональное отбеливание
							img(src='./images/services/9.png')
						a.mobile-menu-services__item.purple-color(href='#')
							p.mobile-menu-services__link Профосмотр
							img(src='./images/services/10.png')
						a.mobile-menu-services__item.light-cyan-color(href='#')
							p.mobile-menu-services__link Детская стоматология
							img(src='./images/services/11.png')
					.mobile-menu__panel.mobile-menu__panel--bottom
						.mobile-menu__panel-top
							a.mobile-menu__phone-link(href="tel:+78352709791") +7 (8352) 70-97-91
							.mobile-menu__social-group
								a.mobile-menu__social.mobile-menu__social--whatsapp(href="#")
								a.mobile-menu__social.mobile-menu__social--viber(href="#")
								a.mobile-menu__social.mobile-menu__social--telegram(href="#")
						a.btn.btn-bg.s-btn.mobile-menu__button(href="#") Онлайн запись
			ul.mobile-menu__menu
				li.mobile-menu__nav-elem
					a.mobile-menu__nav-link.mobile-services-button Услуги
				li.mobile-menu__nav-elem
					a.mobile-menu__nav-link(href="./doctors.html") Врачи
				li.mobile-menu__nav-elem
					a.mobile-menu__nav-link(href="./stocks.html") Акции
				li.mobile-menu__nav-elem
					a.mobile-menu__nav-link(href="./prices.html") Цены
				li.mobile-menu__nav-elem
					.mobile-menu__nav-link-wrap
						a.mobile-menu__nav-link(href="./consultation.html") Пациентам
						button.mobile-menu__nav-link-arrow
					ul.mobile-menu__sub-menu
						li.mobile-menu__nav-sub-elem
							a.mobile-menu__nav-sub-link(href='./portfolio.html') Примеры работ
						li.mobile-menu__nav-sub-elem
							a.mobile-menu__nav-sub-link(href='./reviews.html') Отзывы
						li.mobile-menu__nav-sub-elem
							a.mobile-menu__nav-sub-link(href='./tax-deduction.html') Налоговый вычет
				li.mobile-menu__nav-elem
					a.mobile-menu__nav-link(href="./contacts.html") Адреса клиник
				li.mobile-menu__nav-elem
					a.mobile-menu__nav-link.mobile-menu__nav-link-search Поиск по сайту
		.mobile-menu__panel.mobile-menu__panel--bottom
			.mobile-menu__panel-top
				a.mobile-menu__phone-link(href="tel:+78352709791") +7 (8352) 70-97-91
				.mobile-menu__social-group
					a.mobile-menu__social.mobile-menu__social--whatsapp(href="#")
					a.mobile-menu__social.mobile-menu__social--viber(href="#")
					a.mobile-menu__social.mobile-menu__social--telegram(href="#")
			a.btn.btn-bg.s-btn.mobile-menu__button(href="#") Онлайн запись
		.mobile-menu__panel.mobile-menu__panel--search.mobile-menu-search
			.mobile-menu-search__body.container
				.mobile-menu-search__header
					.mobile-menu-search__input
						input(placeholder='Что будем искать')
						button.mobile-menu-search__input--clean
					button.mobile-menu-search__cancel Отмена
				.mobile-menu-search__answer
